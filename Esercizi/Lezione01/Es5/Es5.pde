void setup()
{
  size(700,300);
}

void draw()
{
  background(0);
  
  ellipse(100,height/2,100,100);
  fill(0);
  ellipse(100+10,height/2-10,100,100);
  fill(255);
  ellipse(200+20,height/2,100,100);
  fill(0);
  ellipse(220+15,height/2-15,100,100);
  fill(255);
  ellipse(300+50,height/2,100,100);
  fill(0);
  ellipse(350+20,height/2-20,100,100);
  fill(255);
  ellipse(400+80,height/2,100,100);
  fill(0);
  ellipse(480+25,height/2-25,100,100);
  fill(255);
  ellipse(500+100,height/2,100,100);
  fill(0);
  ellipse(600+30,height/2-30,100,100);
  fill(255);
  
}
